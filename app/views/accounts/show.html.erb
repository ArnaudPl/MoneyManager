<h2><strong><%= t('.title-part-one') %></strong>: <%= @account.name %></h2>
<div class="row">
  <div class="col s12 m6">
    <div class="card-panel grey lighten-2">
      <p class="balance center"><%= t('.balance') %><br><span class="<%= if @balance.to_f <= 1000 then "red-text" else "green-text" end %>"><%= @balance %></span></p>
    </div>
  </div>
  <div class="col s12 m6">
    <ul class="collection with-header">
      <li class="collection-header"><h4><%= t('.latest-transactions') %></h4></li>
      <%= printAccountLatestTransactions(@account.id).html_safe %>
    </ul>
  </div>
</div>
