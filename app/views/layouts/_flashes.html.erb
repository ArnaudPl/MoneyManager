<% flash.each do |key, value| %>
  <div class="row">
    <div class="col s12 m6 offset-m3">
      <div class="card-panel hoverable center alert alert-<%= key %>">
        <span>
          <%= value %><i class="material-icons right">close</i>
        </span>
      </div>
    </div>
  </div>
<% end %>
